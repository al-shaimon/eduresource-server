# Server Port (Vercel will set this automatically)
PORT=5000

# Node Environment
NODE_ENV=production

# CORS Origins (add your frontend domain here)
CORS_ORIGINS=https://your-frontend-domain.vercel.app,http://localhost:5173

# MongoDB Connection URI (for production use MongoDB Atlas)
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/
# For local development: mongodb://localhost:27017

# Database Name
DB_NAME=eduresource

# JWT Secret Key (change this in production)
JWT_SECRET=jwt-a!@secret-key
